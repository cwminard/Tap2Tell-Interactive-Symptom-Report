<?xml version="1.0" encoding="utf-8"?>
<!--
 This file is used to specify which files and directories to back up.
 See the following link for more information:
 https://developer.android.com/guide/topics/data/autobackup#DefiningRules
-->
<full-backup-content>
    <!--
     By default, all shared preferences are backed up.
     To exclude specific shared preferences, use the <exclude> tag.
     -->
    <exclude domain="sharedpref" path="device.xml"/>
</full-backup-content>
